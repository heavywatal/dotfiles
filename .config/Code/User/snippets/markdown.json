{
  "<div>\\[\\begin{split}": {
    "prefix": "equation",
    "body": [
      "<div>\\\\[\\\\begin{split}",
      "$1",
      "\\\\end{split}\\\\]</div>"
    ]
  },
  "{{< relref >}}": {
    "prefix": "relref",
    "body": [
      "[${1:$TM_SELECTED_TEXT}]({{< relref \"${2:$TM_SELECTED_TEXT}.md\" >}})"
    ]
  }
}