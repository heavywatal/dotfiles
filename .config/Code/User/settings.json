{
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.autoIndent": "advanced",
    "editor.find.seedSearchStringFromSelection": "never",
    "editor.folding": false,
    "editor.fontFamily": "'Ubuntu Mono', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
    "editor.fontSize": 13,
    "editor.hover.sticky": false,
    "editor.lineHeight": 16,
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.quickSuggestionsDelay": 500,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": false,
    "editor.rulers": [80, 160],
    "editor.scrollBeyondLastLine": false,
    "editor.suggest.filterGraceful": false,
    "editor.suggestOnTriggerCharacters": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "comments": "#666"
    },
    "editor.useTabStops": false,
    "editor.wordWrapColumn": 160,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.mustache": "html"
    },
    "files.autoSave": "off",
    "files.trimTrailingWhitespace": true,
    "gitProjectManager.baseProjectsFolders": [
        "~/git"
    ],
    "gitProjectManager.maxDepthRecursion": 1,
    "julia.enableTelemetry": false,
    "julia.enableCrashReporter": false,
    "julia.symbolCacheDownload": false,
    "jupyter.alwaysTrustNotebooks": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.enableCellCodeLens": false,
    "jupyter.searchForJupyter": false,
    "latex-workshop.chktex.args.active": ["-wall", "-n22", "-n30", "-n46", "-e16", "-q"],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.recipe.default": "first",
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.view.pdf.external.synctex.args": [
        "-r",
        "%LINE%",
        "%PDF%",
        "%TEX%"
    ],
    "latex-workshop.view.pdf.external.synctex.command": "displayline",
    "latex-workshop.view.pdf.external.viewer.args": [
        "-a",
        "Skim",
        "%PDF%"
    ],
    "latex-workshop.view.pdf.external.viewer.command": "open",
    "latex-workshop.view.pdf.viewer": "external",
    "python.defaultInterpreterPath": "${env:PYENV_PYTHONPATH}",
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.pythonPath": "${env:PYENV_PYTHONPATH}",
    "python.showStartPage": false,
    "r.rmarkdown.enableCodeLens": false,
    "ruby.format": "rubocop",
    "ruby.intellisense": "rubyLocate",
    "ruby.lint": {
      "rubocop": {
        "command": "/Users/watal/.rbenv/shims/rubocop"
      }
    },
    "ruby.useLanguageServer": true,
    "rust-client.rustupPath": "/Users/watal/.cargo/bin/rustup",
    "spellright.configurationScope": "user",
    "spellright.documentTypes": [
        "latex",
        "markdown"
    ],
    "spellright.language": [
        "en"
    ],
    "spellright.notificationClass": "information",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.defaultProfile.osx": "tmux",
    "terminal.integrated.enableFileLinks": false,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux",
            "args": ["new-session", "-A", "-s", "${workspaceFolderBasename}", "-c", "${workspaceFolder}"]
        }
    },
    "terminal.integrated.tabs.enabled": false,
    "update.mode": "manual",
    "vsicons.dontShowNewVersionMessage": true,
    "window.newWindowDimensions": "inherit",
    "window.openFoldersInNewWindow": "on",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "window.titleBarStyle": "native",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "editorGutter.background": "#222",
        "editorGutter.modifiedBackground": "#c29954",
        "editorOverviewRuler.addedForeground": "#587c0c",
        "editorOverviewRuler.deletedForeground": "#94151b",
        "editorOverviewRuler.findMatchForeground": "#999",
        "editorOverviewRuler.modifiedForeground": "#c29954",
        "editorOverviewRuler.selectionHighlightForeground": "#444",
        "statusBar.background": "#333",
        "terminal.foreground": "#aaa"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "[latex]": {
        "editor.wordWrap": "bounded"
    }
}