{
    "breadcrumbs.showStrings": false,
    "cmake.configureOnEdit": false,
    "cmake.configureOnOpen": false,
    "cmake.ignoreCMakeListsMissing": true,
    "cmake.showOptionsMovedNotification": false,
    "codecov.coverage.enabled": false,
    "cSpell.diagnosticLevel": "Hint",
    "css.format.spaceAroundSelectorSeparator": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.autoIndent": "advanced",
    "editor.detectIndentation": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.seedSearchStringFromSelection": "never",
    "editor.folding": false,
    "editor.fontFamily": "'Ubuntu Mono', 'Apple Color Emoji', 'UbuntuMono Nerd Font', monospace",
    "editor.fontSize": 13,
    "editor.guides.indentation": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.lineHeight": 1.15,
    "editor.lightbulb.enabled": "off",
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": "off",
    "editor.quickSuggestionsDelay": 500,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.renderControlCharacters": true,
    "editor.rulers": [80, 160],
    "editor.scrollBeyondLastLine": false,
    "editor.stickyScroll.enabled": false,
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "comments": "#666"
    },
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "*.mutable": {"underline": false}
        }
    },
    "editor.useTabStops": false,
    "editor.wordWrapColumn": 160,
    "evenBetterToml.schema.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.excludeGitIgnore": true,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.bed": "tsv",
        "*.jbrowse": "json",
        "*.mustache": "html",
    },
    "files.autoSave": "off",
    "files.trimTrailingWhitespace": true,
    "files.trimTrailingWhitespaceInRegexAndStrings": false,
    "github.copilot.chat.githubMcpServer.enabled": true,
    "github.copilot.enable": {
      "markdown": true,
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "gitProjectManager.baseProjectsFolders": [
        "~/git"
    ],
    "gitProjectManager.maxDepthRecursion": 1,
    "gitProjectManager.openInNewWindow": true,
    "gitProjectManager.searchInsideProjects": false,
    "go.formatTool": "gofmt",
    "html.autoClosingTags": false,
    "html.autoCreateQuotes": false,
    "json.format.keepLines": true,
    "json.schemaDownload.trustedDomains": {
      "https://schemastore.azurewebsites.net/": true,
      "https://raw.githubusercontent.com/": true,
      "https://www.schemastore.org/": true,
      "https://json.schemastore.org/": true,
      "https://json-schema.org/": true,
      "https://biomejs.dev/": true,
    },
    "julia.enableTelemetry": false,
    "julia.enableCrashReporter": false,
    "julia.symbolCacheDownload": false,
    "jupyter.alwaysTrustNotebooks": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.interactiveWindow.codeLens.enable": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.recipe.default": "first",
    "latex-workshop.linting.chktex.enabled": true,
    "latex-workshop.linting.chktex.exec.args": ["-wall", "-n19", "-n22", "-n30", "-n46", "-e16", "-q"],
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.view.pdf.external.synctex.args": ["-r", "%LINE%", "%PDF%", "%TEX%"],
    "latex-workshop.view.pdf.external.synctex.command": "displayline",
    "latex-workshop.view.pdf.external.viewer.args": ["-a", "Skim", "%PDF%"],
    "latex-workshop.view.pdf.external.viewer.command": "open",
    "latex-workshop.view.pdf.viewer": "external",
    "makefile.configureOnOpen": false,
    "material-icon-theme.hidesExplorerArrows": true,
    "outline.showArrays": false,
    "outline.showFields": false,
    "outline.showNumbers": false,
    "outline.showVariables": false,
    "python.defaultInterpreterPath": "${env:VIRTUAL_ENV}/bin/python3",
    "python.analysis.typeCheckingMode": "strict",
    "python.showStartPage": false,
    "python.terminal.activateEnvironment": false,
    "python.testing.pytestEnabled": true,
    "r.helpPanel.clickCodeExamples": {
      "Click": "Ignore",
      "Ctrl+Click": "Ignore"
    },
    "r.plot.useHttpgd": false,
    "r.rmarkdown.enableCodeLens": false,
    "r.session.emulateRStudioAPI": false,
    "r.session.viewers.viewColumn": {
      "plot": "Disable"
    },
    "r.sessionWatcher": true,
    "remote.autoForwardPorts": false,
    "remote.restoreForwardedPorts": false,
    "remote.forwardOnOpen": false,
    "rubyLsp.formatter": "standard",
    "rubyLsp.linters": ["standard"],
    "rust-analyzer.lens.enable": false,
    "scss.format.spaceAroundSelectorSeparator": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.enableFileLinks": "notRemote",
    "terminal.integrated.enableImages": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.env.osx": {
        "VSCODE_WORKSPACE_FOLDER": "${workspaceFolder}",
    },
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.macOptionClickForcesSelection": true,
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.scrollback": 65535,
    "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.shellIntegration.history": 0,
    "terminal.integrated.tabs.enabled": false,
    "update.mode": "manual",
    "window.autoDetectColorScheme": true,
    "window.newWindowDimensions": "inherit",
    "window.openFoldersInNewWindow": "on",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "workbench.activityBar.location": "bottom",
    "workbench.colorCustomizations": {
        "editorGutter.background": "#90909010",
        "editorOverviewRuler.findMatchForeground": "#ff666666",
        "editorOverviewRuler.infoForeground": "#368aeba0",
        "editorOverviewRuler.selectionHighlightBackground": "#9999ff99",
        "gitDecoration.modifiedResourceForeground": "#1B81A8",
        "[*Dark*]": {// Selenized Black
          "terminal.ansiBlack": "#252525",
          "terminal.ansiRed": "#ed4a46",
          "terminal.ansiGreen": "#70b433",
          "terminal.ansiYellow": "#dbb32d",
          "terminal.ansiBlue": "#368aeb",
          "terminal.ansiMagenta": "#eb6eb7",
          "terminal.ansiCyan": "#3fc5b7",
          "terminal.ansiWhite": "#777777",
          "terminal.ansiBrightBlack": "#3b3b3b",
          "terminal.ansiBrightRed": "#ff5e56",
          "terminal.ansiBrightGreen": "#83c746",
          "terminal.ansiBrightYellow": "#efc541",
          "terminal.ansiBrightBlue": "#4f9cfe",
          "terminal.ansiBrightMagenta": "#ff81ca",
          "terminal.ansiBrightCyan": "#56d8c9",
          "terminal.ansiBrightWhite": "#dedede",
          "terminal.background": "#181818",
          "terminal.foreground": "#b9b9b9",
          "terminalCursor.foreground": "#dedede",
        },
        "[*Light*]": {// Selenized White
          "terminal.ansiBlack": "#ebebeb",
          "terminal.ansiRed": "#d6000c",
          "terminal.ansiGreen": "#1d9700",
          "terminal.ansiYellow": "#c49700",
          "terminal.ansiBlue": "#0064e4",
          "terminal.ansiMagenta": "#dd0f9d",
          "terminal.ansiCyan": "#00ad9c",
          "terminal.ansiWhite": "#878787",
          "terminal.ansiBrightBlack": "#cdcdcd",
          "terminal.ansiBrightRed": "#bf0000",
          "terminal.ansiBrightGreen": "#008400",
          "terminal.ansiBrightYellow": "#af8500",
          "terminal.ansiBrightBlue": "#0054cf",
          "terminal.ansiBrightMagenta": "#c7008b",
          "terminal.ansiBrightCyan": "#009a8a",
          "terminal.ansiBrightWhite": "#282828",
          "terminal.background": "#ffffff",
          "terminal.foreground": "#474747",
          "terminalCursor.foreground": "#282828",
        }
    },
    "workbench.editor.openSideBySideDirection": "down",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.panel.defaultLocation": "right",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "C_Cpp.autoAddFileAssociations": false,
    "C_Cpp.default.includePath": [
        "~/local/include",
        "~/.R/library/latest/Rcpp/include",
        "~/.R/library/latest/cpp11/include",
        "/Library/Frameworks/R.framework/Headers",
        "/usr/local/include"
    ],
    "C_Cpp.intelliSenseEngine": "disabled",
    "[csv][tsv]": {
        "editor.stickyScroll.enabled": true
    },
    "[go]": {
        "editor.renderWhitespace": "boundary",
        "editor.codeActionsOnSave": {
          "source.organizeImports": "never"
        }
    },
    "[javascript][typescript][json][jsonc]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true,
    },
    "[latex]": {
        "editor.wordWrap": "bounded"
    },
    "[makefile]": {
      "editor.insertSpaces": true
    },
    "[python]": {
        "editor.codeActionsOnSave": {
          "source.fixAll.ruff": "never",
          "source.organizeImports.ruff": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.tabSize": 4,
    },
    "[quarto]": {
        "editor.codeLens": false,
        "editor.wordWrap": "wordWrapColumn",
    },
    "[ruby]": {
      "editor.defaultFormatter": "Shopify.ruby-lsp",
    },
    "[rust]": {
        "editor.tabSize": 4,
    },
}