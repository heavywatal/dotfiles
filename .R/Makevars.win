CXX = $(BINPREF)g++ $(M_ARCH)
CXX11 = $(CXX)
CXX14 = $(CXX)
CXX17 = $(CXX)
CFLAGS = -O2 -Wall $(DEBUGFLAG) -mtune=native
CXXFLAGS = $(CFLAGS)
CXX11FLAGS = $(CFLAGS)
CXX14FLAGS = $(CFLAGS)
CXX17FLAGS = $(CFLAGS)
CXX11STD = -std=c++11
CXX14STD = -std=c++14
CXX17STD = -std=c++17
CPPFLAGS = -IC:/cygwin64/home/$(USERNAME)/local/include -IC:/cygwin64/usr/include
LDFLAGS = -LC:/cygwin64/home/$(USERNAME)/local/lib -LC:/cygwin64/usr/lib
